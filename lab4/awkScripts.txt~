cat cmsc.txt | awk -F',' 'BEGIN {print "Course No. | Section No. | Instructor | Seats | Open | Waitlist | Days | Time, Bldg. | Room No."} /[0-9]{4}\n[A-Za-z]+[\s]{1}[A-Za-z]+/ {print $1, $2, $3, $4;}'

cat cmsc.txt | awk -F',' '/[0-9]{4}/ {print $0;} /\n/ {print $0;} /[A-Za-z]+\s[A-Za-z]+/ {print $0;}'
